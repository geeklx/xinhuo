<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/udesk_color_F6F6F6">
    <TextView
        android:id="@+id/udesk_retry"
        android:layout_width="match_parent"
        android:layout_height="@dimen/udesk_55"
        android:background="@color/white"
        android:textSize="@dimen/udesk_sp18"
        android:gravity="center"
        android:textColor="@color/udesk_color_2A2A2A"
        />
    <TextView
        android:id="@+id/udesk_cancel"
        android:layout_width="match_parent"
        android:layout_height="@dimen/udesk_55"
        android:background="@color/white"
        android:textSize="@dimen/udesk_sp18"
        android:gravity="center"
        android:layout_marginTop="@dimen/udesk_8"
        android:textColor="@color/udesk_color_2A2A2A"
        />
</LinearLayout>